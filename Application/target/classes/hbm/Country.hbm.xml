<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="de.cneubauer.domain.bo.Country" table="country">
        <id name="id" column="Id" >
            <generator class="increment"/>
        </id>
        <property name="name" column="Name" unique="true"/>
        <property name="abbreviation" column="Abbreviation" unique="true" />
        <property name="createdDate" column="CreatedDate" />
        <property name="modifiedDate" column="ModifiedDate" />
    </class>
</hibernate-mapping>