<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="de.cneubauer.domain.bo.DocumentCase" table="documentCase">
        <id name="id" column="Id" >
            <generator class="increment"/>
        </id>
        <property name="caseId" column="Case_ID" />
        <property name="position" column="Position" />
        <property name="isCorrect" column="IsCorrect" />
        <property name="createdDate" column="CreatedDate" />
        <many-to-one name="creditor" class="de.cneubauer.domain.bo.Creditor" column="creditor"/>
        <many-to-one name="keyword" class="de.cneubauer.domain.bo.Keyword" column="keyword" fetch="select"/>
    </class>
</hibernate-mapping>