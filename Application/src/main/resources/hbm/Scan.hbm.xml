<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="de.cneubauer.domain.bo.Scan" table="scan">
        <id name="id" column="Id" >
            <generator class="increment"/>
        </id>
        <property name="file" column="File" />
        <property name="createdDate" column="CreatedDate" />
        <property name="modifiedDate" column="ModifiedDate" />
        <many-to-one  name="invoiceInformation" class="de.cneubauer.domain.bo.Invoice" column="InvoiceInformation" cascade="save-update"/>
    </class>
</hibernate-mapping>