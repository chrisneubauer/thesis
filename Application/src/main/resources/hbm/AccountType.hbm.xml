<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="de.cneubauer.domain.bo.Address" table="address">
        <id name="id" column="Id" >
            <generator class="increment"/>
        </id>
        <property name="street" column="Street" />
        <property name="zipCode" column="ZipCode" />
        <property name="city" column="City" />
        <property name="createdDate" column="CreatedDate" />
        <property name="modifiedDate" column="ModifiedDate" />
        <many-to-one name="country" class="de.cneubauer.domain.bo.Country" column="Country"/>
    </class>
</hibernate-mapping>