<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="de.cneubauer.gui.controller.DatabaseResultsController">
    <fx:include source="menubar.fxml" />
    <AnchorPane VBox.vgrow="ALWAYS">
        <GridPane layoutX="7.0" layoutY="7.0" prefHeight="375.0" prefWidth="640.0" AnchorPane.bottomAnchor="0.0"
                  AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <ButtonBar buttonMinWidth="120.0" prefHeight="40.0" prefWidth="200.0" GridPane.columnIndex="10"
                       GridPane.rowIndex="1">
                <buttons>
                    <Button fx:id="ReturnToDatabaseSearch" mnemonicParsing="false"
                            onAction="#returnToDatabaseSearch" text="%showDatabaseResults.button.returnToDatabaseSearch"/>
                </buttons>
                <GridPane.margin>
                    <Insets/>
                </GridPane.margin>
            </ButtonBar>
            <TableView fx:id="dbResultList" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1"
                       GridPane.columnSpan="12" GridPane.rowIndex="2" GridPane.rowSpan="7">
                <columns>
                    <TableColumn fx:id="dateColumn" editable="false" prefWidth="75.0" text="%showDatabaseResults.tablecolumn.date"/>
                    <TableColumn fx:id="valueColumn" editable="false" prefWidth="75.0" text="%showDatabaseResults.tablecolumn.value"/>
                    <TableColumn fx:id="creditorColumn" editable="false" prefWidth="75.0" text="%showDatabaseResults.tablecolumn.creditor"/>
                    <TableColumn fx:id="debitorColumn" editable="false" prefWidth="75.0" text="%showDatabaseResults.tablecolumn.debitor"/>
                    <TableColumn fx:id="downloadColumn" editable="false" prefWidth="75.0" sortable="false"/>
                </columns>
            </TableView>
            <Label text="%showDatabaseResults.label.foundDatasets" GridPane.columnIndex="1"
                   GridPane.columnSpan="6" GridPane.rowIndex="1"/>
        </GridPane>
    </AnchorPane>
</VBox>
