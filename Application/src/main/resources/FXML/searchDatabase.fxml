            <DatePicker fx:id="date" prefHeight="25.0" prefWidth="175.0" GridPane.columnIndex="4"
                        GridPane.columnSpan="4" GridPane.rowIndex="2"/>
            <Label text="Debitor:" GridPane.columnIndex="2" GridPane.rowIndex="3"/>
