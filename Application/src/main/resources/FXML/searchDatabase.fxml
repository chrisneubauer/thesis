            <TextField fx:id="creditor" GridPane.columnIndex="4" GridPane.columnSpan="4" GridPane.rowIndex="4"/>
            <Label text="Value:" GridPane.columnIndex="2" GridPane.rowIndex="5"/>
            <TextField fx:id="value" GridPane.columnIndex="4" GridPane.columnSpan="4" GridPane.rowIndex="5"/>
            <ButtonBar buttonMinWidth="120.0" prefHeight="40.0" prefWidth="200.0" GridPane.columnIndex="5"
